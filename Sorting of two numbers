import java. io. *;
import java. util. *;
public class Main
{
	public static void main(String args[]) 
	{
		int a=10, b=9;
		System.out.println("before sorting") ;
		System.out.println("value of a:"+a) ;
		System.out.println("value of b:"+b) ;
		{
			a=a+b;
			b=a-b;
			a=a-b;
			}
			System.out.println("after sorting") ;
			System.out.println("value of a:"+a) ;
			System.out.println("value of b:"+b) ;
			}
			}
			

